{
  "singleQuote": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "trailingComma": "es5",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
