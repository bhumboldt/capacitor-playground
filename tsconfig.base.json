      "@cockpit/distance": ["libs/mobile/shared/utils/distance/src/index.ts"],
      "@cockpit/environment": ["libs/mobile/shared/environment/src/index.ts"],
      "@cockpit/event-page": ["libs/mobile/user/event-page/src/index.ts"],
      "@cockpit/http": ["libs/mobile/shared/http/src/index.ts"],
      "@cockpit/network": ["libs/mobile/shared/network/src/index.ts"],
      "@cockpit/network-state": ["libs/mobile/state/network/src/index.ts"],
      "@cockpit/network-sync": [
        "libs/mobile/effects/network-sync/src/index.ts"
      ],
      "@cockpit/photo-method-picker": [
        "libs/mobile/ui/photo-method-picker/src/index.ts"
      ],
      "@cockpit/selfie-modal": ["libs/mobile/modals/selfie-modal/src/index.ts"],
      "@cockpit/selfies-effects": ["libs/mobile/effects/selfies/src/index.ts"],
      "@cockpit/selfies-state": ["libs/mobile/state/selfies/src/index.ts"],
      "@cockpit/sqlite": ["libs/mobile/shared/sqlite/src/index.ts"],
      "@cockpit/storage": ["libs/mobile/shared/storage/src/index.ts"],
      "@cockpit/tracking": ["libs/mobile/shared/tracking/src/index.ts"],
      "@cockpit/tracking-effects": [
        "libs/mobile/effects/tracking/src/index.ts"
      ],
      "@cockpit/tracking-state": ["libs/mobile/state/tracking/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
